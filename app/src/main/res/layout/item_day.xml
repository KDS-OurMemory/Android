<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:id="@+id/ll_item_day_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/calendar_edge"
    android:gravity="center|top"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_item_day_text"
        android:layout_width="wrap_content"
        android:layout_height="16dp"
        android:layout_marginBottom="3dp"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="10dp"
        tool:ignore="SpUsage"
        tool:text="00" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/ll_item_day_calendar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintTop_toTopOf="parent">

            <LinearLayout
                android:id="@+id/ll_item_day_event_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="3dp"
                android:orientation="vertical"
                android:visibility="visible">

                <LinearLayout
                    android:id="@+id/ll_item_day_birthday_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:visibility="gone"
                    tool:ignore="UseCompoundDrawables,UselessParent">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:src="@drawable/ic_baseline_cake_24"
                        tool:ignore="ContentDescription" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="5dp"
                        android:text="생일"
                        android:textColor="@color/black"
                        android:textSize="10dp"
                        android:textStyle="bold"
                        tool:ignore="HardcodedText,SpUsage" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_item_day_calendar1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="3dp"
                android:orientation="horizontal"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/tv_item_day_calendar_text1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:paddingStart="3dp"
                    android:textColor="@color/black"
                    android:textSize="10dp"
                    tool:ignore="RtlSymmetry,SpUsage" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_item_day_calendar2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="3dp"
                android:orientation="horizontal"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/tv_item_day_calendar_text2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:paddingStart="3dp"
                    android:textColor="@color/black"
                    android:textSize="10dp"
                    tool:ignore="RtlSymmetry,SpUsage" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_item_day_calendar3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="3dp"
                android:orientation="horizontal"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/tv_item_day_calendar_text3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:paddingStart="3dp"
                    android:textColor="@color/black"
                    android:textSize="10dp"
                    tool:ignore="RtlSymmetry,SpUsage" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_item_day_calendar4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="2dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="3dp"
                android:orientation="horizontal"
                android:visibility="invisible">

                <TextView
                    android:id="@+id/tv_item_day_calendar_text4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:paddingStart="3dp"
                    android:textColor="@color/black"
                    android:textSize="10dp"
                    tool:ignore="RtlSymmetry,SpUsage" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_item_day_calendar_text_etc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:paddingStart="3dp"
                    android:textColor="@color/black"
                    android:textSize="10dp"
                    tool:ignore="RtlSymmetry,SpUsage" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_item_day_dot_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/iv_item_day_dot_image1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="1dp"
                android:visibility="invisible"
                tool:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/iv_item_day_dot_image2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="1dp"
                android:visibility="invisible"
                tool:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/iv_item_day_dot_image3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="1dp"
                android:visibility="invisible"
                tool:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/iv_item_day_dot_image4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="1dp"
                android:visibility="invisible"
                tool:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/iv_item_day_dot_image5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="1dp"
                android:layout_marginEnd="1dp"
                android:visibility="invisible"
                tool:ignore="ContentDescription" />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</LinearLayout>